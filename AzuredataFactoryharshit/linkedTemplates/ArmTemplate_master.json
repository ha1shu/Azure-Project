{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AzuredataFactoryharshit"
		},
		"AzureDataLake_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLake_ls'"
		},
		"Azure_Sql_Ls_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Azure_Sql_Ls'"
		},
		"AzureDataLake_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageaccountharshit.dfs.core.windows.net/"
		},
		"Azure_Sql_Ls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "azureserverspotify.database.windows.net"
		},
		"Azure_Sql_Ls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "projectDB"
		},
		"Azure_Sql_Ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminharshit"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AzuredataFactoryharshit_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLake_ls_accountKey": {
						"value": "[parameters('AzureDataLake_ls_accountKey')]"
					},
					"Azure_Sql_Ls_password": {
						"value": "[parameters('Azure_Sql_Ls_password')]"
					},
					"AzureDataLake_ls_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLake_ls_properties_typeProperties_url')]"
					},
					"Azure_Sql_Ls_properties_typeProperties_server": {
						"value": "[parameters('Azure_Sql_Ls_properties_typeProperties_server')]"
					},
					"Azure_Sql_Ls_properties_typeProperties_database": {
						"value": "[parameters('Azure_Sql_Ls_properties_typeProperties_database')]"
					},
					"Azure_Sql_Ls_properties_typeProperties_userName": {
						"value": "[parameters('Azure_Sql_Ls_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}